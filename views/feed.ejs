            <div class="card-body">
              <h5 class="card-title">Cai da escada ontem</h5>
              <p class="card-text">Meu pai me empurrou</p>
              <button type="button" class="btn btn-primary">Like</button>
              <button type="button" class="btn btn-danger">Dislike</button>
            </div>
          </div>
        </div>

        <% } %>
      </div>
  </main>
</body>
</html>